  ],
  "author": "Andreas Reich (https://github.com/cyraxx)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cyraxx/node-pogo-protos/issues"
  },
  "homepage": "https://github.com/cyraxx/node-pogo-protos#readme",
  "dependencies": {
    "protobufjs": "^6.8.0"
  },
  "typings": "./pogo-protos.d.ts",
  "devDependencies": {
    "mz": "^2.6.0"
